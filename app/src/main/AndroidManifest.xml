<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.codecorp.insuranceuser">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".RcActivity"></activity>
        <activity android:name=".DocumentsActivity" />
        <activity android:name=".RequestActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".WorkshopActivity" />
        <activity android:name=".NewVehicleActivity" />
        <activity android:name=".VehicleDetailsActivity" />
        <activity
            android:name=".WorkshopDashboard"
            android:label="@string/title_activity_workshop_dashboard" />
        <activity
            android:name=".UserDashboard"
            android:label="@string/title_activity_user_dashboard" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>